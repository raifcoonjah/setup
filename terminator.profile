[global_config]
  ask_before_closing = never
[keybindings]
  copy = <Primary>c
  paste = <Primary>v
[profiles]
  [[default]]
    allow_bold = False
    font = SFMono Nerd Font Medium 10.199
    show_titlebar = False
    scrollbar_position = hidden
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
